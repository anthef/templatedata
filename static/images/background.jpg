<svg viewBox="0 0 1200 800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#f0f4ff" />
      <stop offset="100%" stop-color="#e0e7ff" />
    </linearGradient>
    <pattern id="dots" width="40" height="40" patternUnits="userSpaceOnUse">
      <circle cx="20" cy="20" r="2" fill="#6366f1" fill-opacity="0.3" />
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#bg-gradient)" />
  <rect width="100%" height="100%" fill="url(#dots)" />
  
  <!-- Decorative Elements -->
  <circle cx="100" cy="100" r="80" fill="#6366f1" fill-opacity="0.1" />
  <circle cx="1100" cy="700" r="120" fill="#8b5cf6" fill-opacity="0.1" />
  
  <!-- Abstract Shapes -->
  <path d="M0,400 Q300,300 600,500 T1200,400" stroke="#6366f1" stroke-width="3" fill="none" stroke-opacity="0.2" />
  <path d="M0,500 Q300,600 600,400 T1200,500" stroke="#8b5cf6" stroke-width="3" fill="none" stroke-opacity="0.2" />
  
  <!-- Data Nodes -->
  <g transform="translate(200, 200)">
    <circle cx="0" cy="0" r="15" fill="#6366f1" />
    <circle cx="100" cy="50" r="10" fill="#8b5cf6" />
    <circle cx="200" cy="100" r="12" fill="#6366f1" />
    <circle cx="300" cy="50" r="8" fill="#8b5cf6" />
    <circle cx="400" cy="0" r="15" fill="#6366f1" />
    <line x1="0" y1="0" x2="100" y2="50" stroke="#6366f1" stroke-width="2" />
    <line x1="100" y1="50" x2="200" y2="100" stroke="#6366f1" stroke-width="2" />
    <line x1="200" y1="100" x2="300" y2="50" stroke="#6366f1" stroke-width="2" />
    <line x1="300" y1="50" x2="400" y2="0" stroke="#6366f1" stroke-width="2" />
  </g>
  
  <!-- Tech Icons -->
  <g transform="translate(800, 200)">
    <rect x="0" y="0" width="60" height="60" rx="10" fill="#6366f1" fill-opacity="0.2" />
    <rect x="80" y="0" width="60" height="60" rx="10" fill="#8b5cf6" fill-opacity="0.2" />
    <rect x="160" y="0" width="60" height="60" rx="10" fill="#6366f1" fill-opacity="0.2" />
    <rect x="0" y="80" width="60" height="60" rx="10" fill="#8b5cf6" fill-opacity="0.2" />
    <rect x="80" y="80" width="60" height="60" rx="10" fill="#6366f1" fill-opacity="0.2" />
    <rect x="160" y="80" width="60" height="60" rx="10" fill="#8b5cf6" fill-opacity="0.2" />
    
    <!-- Python Symbol -->
    <g transform="translate(20, 30)">
      <path d="M10,0 A10,10 0 0 1 10,20 A10,10 0 0 1 10,0" fill="#fff" />
      <path d="M20,0 A10,10 0 0 1 20,20 A10,10 0 0 1 20,0" fill="#fff" />
    </g>
  </g>
  
  <!-- Bottom Decorative Wave -->
  <path d="M0,750 C200,790 400,710 600,750 C800,790 1000,710 1200,750" stroke="#6366f1" stroke-width="5" fill="none" stroke-opacity="0.3" />
</svg>